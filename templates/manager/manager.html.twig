{% extends 'base.html.twig' %}

{% block title %}Hello ManagerController!{% endblock %}

{% block body %}
<h1> Unclaimed Tickets</h1>
    <table>
        <tr>
            <th>Priority</th>
            <th>Status</th>
            <th>Title</th>
            <th>Made</th>
        </tr>
        {% for ticket in allMadeTickets  %}
            <tr>
                <td>{{ ticket.priority }}</td>
                <td>{{ ticket.status }}</td>
                <td>{{ ticket.title }}</td>
                <td>{{ ticket.time|date("m-d H:i") }}</td>
            </tr>
        {% endfor %}
    </table>
<h1>In Progress</h1>
<table>
    <tr>
        <th>Priority</th>
        <th>Status</th>
        <th>Title</th>
        <th>Made</th>
        <th>Agent Assigned</th>
    </tr>
